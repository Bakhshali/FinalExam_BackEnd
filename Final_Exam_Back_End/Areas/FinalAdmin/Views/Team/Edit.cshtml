@model Team
@{
    ViewData["Title"] = "Edit";
}

<div class="m-5">
    <form method="post" enctype="multipart/form-data">
        <div>
            <input class="form-control" asp-for="Name" placeholder="Name" /> <br />
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>
        <div>
            <input class="form-control" asp-for="Position" placeholder="Position" /> <br />
            <span asp-validation-for="Position" class="text-danger"></span>
        </div>
        <div>
            <input class="form-control" asp-for="Subtitle" placeholder="Subtitle" /> <br />
            <span asp-validation-for="Subtitle" class="text-danger"></span>
        </div>
        <div class="mt-3">
            <input class="form-control" asp-for="FbUrl" placeholder="Facebook URL" />
        </div>
        <div class="mt-3">
            <input class="form-control" asp-for="TwitterUrl" placeholder="Twitter URL" />
        </div>
        <div class="mt-3">
            <input class="form-control" asp-for="InstaUrl" placeholder="Instagram URL" />
        </div>
        <div class="mt-5">
            <input asp-for="Photo" /> <br />
            <span asp-validation-for="Photo" class="text-danger"></span>
        </div>
        <div>
            <button type="submit" class="btn btn-success mt-4">Edit</button>
        </div>
    </form>
</div>

